<?xml version='1.0' encoding='utf-8'?>
<widget id="com.idtklb98jlgpxvqka0jl" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>practice_everyday2</name>
    <description>New Meteor Mobile App</description>
    <author email="n/a" href="n/a">A Meteor Developer</author>
    <preference name="webviewbounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="5000" />
    <preference name="FadeSplashScreenDuration" value="250" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarStyle" value="default" />
    <content src="http://localhost:12976/" />
    <allow-intent href="tel:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="market:*" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <allow-navigation href="http://localhost" />
    <access origin="*" />
    <platform name="ios">
        <icon height="120" src="resources/iphone_2x.icon.png" width="120" />
        <icon height="180" src="resources/iphone_3x.icon.png" width="180" />
        <icon height="76" src="resources/ipad.icon.png" width="76" />
        <icon height="152" src="resources/ipad_2x.icon.png" width="152" />
        <icon height="167" src="resources/ipad_pro.icon.png" width="167" />
        <icon height="29" src="resources/ios_settings.icon.png" width="29" />
        <icon height="58" src="resources/ios_settings_2x.icon.png" width="58" />
        <icon height="40" src="resources/ios_spotlight.icon.png" width="40" />
        <icon height="80" src="resources/ios_spotlight_2x.icon.png" width="80" />
        <splash height="960" src="resources/iphone_2x.splash.png" width="640" />
        <splash height="1136" src="resources/iphone5.splash.png" width="640" />
        <splash height="1334" src="resources/iphone6.splash.png" width="750" />
        <splash height="2208" src="resources/iphone6p_portrait.splash.png" width="1242" />
        <splash height="1242" src="resources/iphone6p_landscape.splash.png" width="2208" />
        <splash height="1024" src="resources/ipad_portrait.splash.png" width="768" />
